{
    "name": "viart/dashboard",
    "description": "A dashboard for Laravel",
    "type": "library",
    "keywords": [
        "dashboard",
        "laravel-dashboard"
    ],
    "homepage": "https://github.com/Intiligent/dashboard",
    "license": "MIT",
    "authors": [
        {
            "name": "Vitaliy Artyukh",
            "email": "vitaliy.artyukh@gmail.com",
            "homepage": "https://www.linkedin.com/in/vitaliy-artyukh/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "coderello/laravel-shared-data": "^3.0",
        "eusonlito/laravel-meta": "3.1.*",
        "tightenco/ziggy": "^0.9.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Dashboard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dashboard\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dashboard\\DashboardServiceProvider"
            ],
            "aliases": {
                "Dashboard": "Dashboard\\Facades\\Dashboard"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
