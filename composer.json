{
    "name": "viart/dashboard",
    "description": "A dashboard for Laravel",
    "type": "library",
    "keywords": [
        "dashboard",
        "dashboard-panel",
        "laravel-dashboard",
        "laravel",
        "cms",
        "admin",
        "admin-panel",
        "panel"
    ],
    "homepage": "https://github.com/Intiligent/dashboard",
    "support": {
        "issues": "https://github.com/Intiligent/dashboard/issues",
        "email": "vitaliy.artyukh@gmail.com"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Vitaliy Artyukh",
            "email": "vitaliy.artyukh@gmail.com",
            "homepage": "https://www.linkedin.com/in/vitaliy-artyukh/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "coderello/laravel-shared-data": "^3.0",
        "eusonlito/laravel-meta": "3.1.*",
        "kalnoy/nestedset": "^5.0",
        "spatie/laravel-permission": "^4.2",
        "tightenco/ziggy": "^0.9.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "viart\\dashboard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "viart\\dashboard\\tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "viart\\dashboard\\DashboardServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
